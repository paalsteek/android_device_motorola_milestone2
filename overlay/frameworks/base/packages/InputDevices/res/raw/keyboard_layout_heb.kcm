	shift+alt: '<'
	ctrl, meta: none
}
key COMMA { 
	label: '\u05EA'
	number: ','
	base: '\u05EA'
	shift, capslock: ','
	alt, numlock: ';'
	shift+alt: ';'
	ctrl, meta: none
}
key PERIOD { 
	label: '.'
	number: ','
	base: '.'
	shift, capslock: ','
	alt, numlock: ':'
	shift+alt: ':'
	ctrl, meta: none
}
key POUND { 
	label: '\u20ac' 
	number: '~'
	base: '\u20ac'  
	shift, capslock: '\u20ac'
	alt, numlock: '~'
	shift+alt: '~'
	ctrl, meta: none
}
key SPACE { 
	label: ' '
	number: ' ' 
	base: ' '
	shift, capslock: ' '
	alt, numlock: '\uEF01' 
	shift+alt: '\uEF01' 
	ctrl, meta: none
}
key ENTER { 
    label: '\n'
    base:  '\n'
    ctrl, alt, numlock, capslock, meta: none
}
key GRAVE { 
	label: '`'
	number: '`'
	base: '`'
	shift, capslock: '~'
	alt, numlock: '`'
	shift+alt: '~'
	ctrl, meta: none
}

key SEMICOLON { 
	label: ';'
	number: ';'
	base: ';'
	shift, capslock: ';'
	alt, numlock: ';'
	shift+alt: ';'
	ctrl, meta: none
}

key SLASH { 
	label: '/'
	number: '/'
	base: '/'
	shift, capslock: '/'
	alt, numlock: '?'
	shift+alt: '?'
	ctrl, meta: none
}

key AT { 
	label: '@'
	number: '^'
	base: '@'
	shift, capslock: '@'
	alt, numlock: '^'
	shift+alt: '^'
	ctrl, meta: none
}

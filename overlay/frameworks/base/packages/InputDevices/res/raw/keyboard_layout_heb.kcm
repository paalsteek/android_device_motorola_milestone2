#
# Hebrew QWERTY keyboard layout.
# For Motorola Milestone 2
#

type OVERLAY

key A { 
	label: '\u05E9'
	number: '!'
	base: '\u05E9'
	shift, capslock: 'A'
	alt, numlock: '!'
	shift+alt: '!'
	ctrl, meta: none
}
key B { 
	label: '\u05E0'
	number: '+'
	base: '\u05E0'
	shift, capslock: 'B'
	alt, numlock: '+'
	shift+alt: '+'
	ctrl, meta: none
}
key C { 
	label: '\u05D1'
	number: '_'
	base: '\u05D1'
	shift, capslock: 'C'
	alt, numlock: '_'
	shift+alt: '_'
	ctrl, meta: none
}
key D { 
	label: '\u05D2'
	number: '$'
	base: '\u05D2'
	shift, capslock: 'D'
	alt, numlock: '$'
